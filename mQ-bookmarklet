javascript:(function(){"use strict";let isQlickOn;let marker=document.getElementById('qlick');if(marker){document.body.removeChild(marker);isQlickOn=false}else{let el=document.createElement("div");el.id="qlick";el.innerText="Qlick";el.style.position="fixed";el.style.bottom="15px";el.style.left="15px";el.style.color="#666";el.style.border="1px solid #aaa";el.style.borderRadius="5px";el.style.padding="3px 7px";el.style.zIndex=9999;document.body.appendChild(el);isQlickOn=true}document.onmousedown=function(e){if(isQlickOn&&e.which==2){let target;if(e.target.tagName=="A"&&e.target.href){target=e.target}else{let ancestor=e.target.closest('a[href]');if(ancestor.href){target=ancestor}}if(target){let originalURL=target.href;let url=target.href;let prevLength=url.length;let args=[["/groups/","/"],["/posts/","?"],["/media/set/","&"],["/photo.php?","&"],[".php?story_fbid=","&notif"],["/login_alerts/","&"],["onthisday/message","&creator"],[".com/","?"],];for(let c=0;c<args.length;++c){url=url.split(args[c][0]).length==2?(url.split(args[c][0])[0]+args[c][0]+url.split(args[c][0])[1].split(args[c][1])[0]):url;if(url.length<prevLength)break}target.href=url;setTimeout(function(){target.href=originalURL},500)}}}})()